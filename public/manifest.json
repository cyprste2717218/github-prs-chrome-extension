{
  "manifest_version": 3,
  "name": "Github Pull Request Tracker",
  "version": "1.0",
  "description": "See at a glance how many pull requests are open across your tracked repositories!",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["scripting", "tabs", "activeTab", "storage"],
  "host_permissions": ["https://*/*", "http://*/*"]
}
